{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "C:\\Temp\\TestDocuments\\async-log.json",
                "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                "buffered": true,
                "flushToDiskInterval": "TimeSpan.FromMilliseconds(1000)",
                "blockWhenFull": true
              }
            }
          ]
        }
      }
    ]
  }
}
